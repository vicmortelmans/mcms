<?xml version="1.0"?>

<font-mappings>
   <font-table>

      <!-- Backwards compatibility aliases -->
      <aliases><alias name="serif">Serif</alias></aliases>
      <aliases><alias name="sans-serif">Sans</alias></aliases>
      <aliases><alias name="monospace">Monospaced</alias></aliases>
      <aliases><alias name="Helvetica">Normal</alias></aliases>
      <aliases><alias name="Courier">Monospaced</alias></aliases>

      <logical-font name="Sans">
         <physical-font char-set="default"><font-face>Noto Sans</font-face></physical-font>
         <physical-font char-set="Cyrillic"><font-face>Noto Sans</font-face></physical-font>
         <physical-font char-set="Traditional Chinese"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Traditional Chinese - Ext B"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Simplified Chinese"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Japanese"><font-face>MS PGothic</font-face></physical-font>
         <physical-font char-set="Korean"><font-face>Gungsuh</font-face></physical-font>
         <physical-font char-set="Thai"><font-face>Arial Unicode MS</font-face></physical-font>
         <physical-font char-set="Greek"><font-face>Noto Sans</font-face></physical-font>
         <physical-font char-set="Latin-extended"><font-face>Noto Sans</font-face></physical-font>
         <physical-font char-set="Symbols"><font-face>ZapfDingbats</font-face></physical-font>
         <physical-font char-set="SubmenuSymbol"><font-face>ZapfDingbats</font-face></physical-font>
         <physical-font char-set="SymbolsSuperscript"><font-face>Arial Unicode MS</font-face><baseline-shift>20%</baseline-shift><override-size>smaller</override-size></physical-font>
      </logical-font>

      <logical-font name="Serif">
         <physical-font char-set="default"><font-face>Noto Serif</font-face></physical-font>
         <physical-font char-set="Cyrillic"><font-face>Noto Serif</font-face></physical-font>
         <physical-font char-set="Traditional Chinese"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Traditional Chinese - Ext B"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Simplified Chinese"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Japanese"><font-face>MS PMincho</font-face></physical-font>
         <physical-font char-set="Korean"><font-face>Batang</font-face></physical-font>
         <physical-font char-set="Thai"><font-face>Arial Unicode MS</font-face></physical-font>
         <physical-font char-set="Greek"><font-face>Noto Serif</font-face></physical-font>
         <physical-font char-set="Latin-extended"><font-face>Noto Serif</font-face></physical-font>
         <physical-font char-set="Symbols"><font-face>ZapfDingbats</font-face></physical-font>
         <physical-font char-set="SubmenuSymbol"><font-face>ZapfDingbats</font-face></physical-font>
         <physical-font char-set="SymbolsSuperscript"><font-face>Times New Roman</font-face><baseline-shift>20%</baseline-shift><override-size>smaller</override-size></physical-font>
      </logical-font>

      <logical-font name="Monospaced">
         <physical-font char-set="default"><font-face>Courier</font-face></physical-font>
         <physical-font char-set="Cyrillic"><font-face>DejaVuLGCSansMono</font-face></physical-font>
         <physical-font char-set="Traditional Chinese"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Traditional Chinese - Ext B"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Simplified Chinese"><font-face>Sazanami-Hanazono Mincho</font-face></physical-font>
         <physical-font char-set="Japanese"><font-face>MS Gothic</font-face></physical-font>
         <physical-font char-set="Korean"><font-face>GungsuhChe</font-face></physical-font>
         <physical-font char-set="Thai"><font-face>Arial Unicode MS</font-face></physical-font>
         <physical-font char-set="Greek"><font-face>DejaVuLGCSansMono</font-face></physical-font>
         <physical-font char-set="Latin-extended"><font-face>DejaVuLGCSansMono</font-face></physical-font>
         <physical-font char-set="Symbols"><font-face>ZapfDingbats</font-face></physical-font>
         <physical-font char-set="SymbolsSuperscript"><font-face>Courier</font-face><baseline-shift>20%</baseline-shift><override-size>smaller</override-size></physical-font>
      </logical-font>
   </font-table>
</font-mappings>
